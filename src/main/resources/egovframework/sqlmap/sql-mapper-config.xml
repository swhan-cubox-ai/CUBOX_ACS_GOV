<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
	<settings>
		<setting name="mapUnderscoreToCamelCase" value="false"/>
		<setting name="callSettersOnNulls" value="false"/>			<!-- 쿼리 결과 필드가 null인 경우, 누락이 되서 나오는데 누락이 안되게 하는 설정 -->
		<setting name="jdbcTypeForNull" value="NULL"/>				<!-- 쿼리에 보내는 파라메터가 null인 경우, 오류 발생하는 것 방지  예) #{search.user}  -->
	</settings>
	<typeAliases>
		<typeAlias alias="egovMap" type="egovframework.rte.psl.dataaccess.util.EgovMap"/>
		<typeAlias alias="authorVO" type="aero.cubox.auth.service.vo.AuthorVO"/>
		<typeAlias alias="siteVO" type="aero.cubox.sample.service.vo.SiteVO"/>
		<typeAlias alias="menuClVO" type="aero.cubox.menu.vo.MenuClVO"/>
		<typeAlias alias="menuDetailVO" type="aero.cubox.menu.vo.MenuDetailVO"/>
		<typeAlias alias="commonUtils" type="aero.cubox.util.CommonUtils"/>
		<typeAlias alias="logInfoVO" type="aero.cubox.sample.service.vo.LogInfoVO"/>
		<typeAlias alias="mainStatusVO" type="aero.cubox.sample.service.vo.MainStatusVO"/>
		<typeAlias alias="authorGroupVO" type="aero.cubox.auth.service.vo.AuthorGroupVO"/>
		<typeAlias alias="authorGroupDetailVO" type="aero.cubox.auth.service.vo.AuthorGroupDetailVO"/>
		<typeAlias alias="syncDeviceVO" type="aero.cubox.sample.service.vo.SyncDeviceVO"/>
		<typeAlias alias="userInfoVO" type="aero.cubox.user.service.vo.UserInfoVO"/>
		<typeAlias alias="cardInfoVO" type="aero.cubox.sample.service.vo.CardInfoVO"/>
		<typeAlias alias="userChgLogVO" type="aero.cubox.user.service.vo.UserChgLogVO"/>
		<typeAlias alias="excelVO" type="aero.cubox.sample.service.vo.ExcelVO"/>
	</typeAliases>
</configuration>